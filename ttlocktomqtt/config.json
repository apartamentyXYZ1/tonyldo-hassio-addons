{
  "name": "TTLockToMqtt",
  "version": "0.1",
  "slug": "ttlocktomqtt",
  "description": "Adapter between ttlock cloud api and mqtt server",
  "arch": [
    "armhf",
    "armv7",
    "aarch64",
    "amd64",
    "i386"
  ],
  "url": "https://github.com/tonyldo/TTLockToMQTT",
  "startup": "system",
  "boot": "auto",
  "options": {
    "ttlockclientapp": "TTLOCK CLOUD API CLIENT",
    "ttlocktoken": "TTLOCK TOKEN",
    "mqttbrokerhost": "localhost",
    "mqttbrokerport": "1883",
    "mqttbrokeruser": "mqtt user",
    "mqttbrokerpass": "mqtt pass"
  },
  "schema": {
    "ttlockclientapp": "str",
    "ttlocktoken":"str",
    "mqttbrokerhost": "str?",
    "mqttbrokerport": "port?",
    "mqttbrokeruser": "str?",
    "mqttbrokerpass": "str?"
  }
}